{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/search", "dest": "/api/search.js" },
    { "src": "/product-details", "dest": "/api/product-details.js" },
    { "src": "/download", "dest": "/api/download.js" },
    { "src": "/", "dest": "/api/index.js" }
  ]
}
