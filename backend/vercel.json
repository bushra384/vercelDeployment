{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/search",
      "dest": "api/main.js"
    },
    {
      "src": "/product-details/(.*)",
      "dest": "api/main.js"
    },
    {
      "src": "/download",
      "dest": "api/main.js"
    }
  ]
}
